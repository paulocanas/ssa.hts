# create an hankel matrix
hankel_matrix <- function(n, m) {
# create a matrix of zeros
matrix <- matrix(0, nrow = n, ncol = m)
# fill the matrix with the hankel pattern
for (i in 1:n) {
for (j in 1:m) {
if (i + j <= n + 1) {
matrix[i, j] <- i + j - 1
}
}
}
return(matrix)
}
# test the function
hankel_matrix(5, 5)
# Transform a vector in an hankel matrix
hankel_matrix <- function(x, L) {
n <- length(x)
m <- n - L + 1
H <- matrix(0, nrow = L, ncol = m)
for (i in 1:L) {
for (j in 1:m) {
H[i, j] <- x[i + j - 1]
}
}
return(H)
}
# Teste the function
x <- 1:100
L <- 12
hankel_matrix(x, L)
# Transform a hankel matrix in a vector
hankel_vector <- function(H) {
L <- nrow(H)
m <- ncol(H)
x <- numeric(L + m - 1)
for (i in 1:L) {
x[i] <- H[i, 1]
}
for (i in 2:m) {
x[L + i - 1] <- H[L, i]
}
return(x)
}
# Teste the function
H <- hankel_matrix(x, L)
hankel_vector(H)
